{
    "name": "rishadblack/i-reports",
    "description": ":Reusable and extensible Laravel reporting package featuring dynamic report loading, Livewire-driven filters, export to PDF and Excel using blade views, and support for modular applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "S M Rishad",
            "email": "rishad.black@gmail.com",
            "homepage": "https://smrishad.com"
        }
    ],
    "homepage": "https://github.com/rishadblack/i-reports",
    "keywords": [
        "Laravel",
        "IReports"
    ],
    "version": "0.1.0",
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "maatwebsite/excel": "^3.1",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Rishadblack\\IReports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rishadblack\\IReports\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rishadblack\\IReports\\IReportsServiceProvider"
            ],
            "aliases": {
                "IReports": "Rishadblack\\IReports\\Facades\\IReports"
            }
        }
    }
}